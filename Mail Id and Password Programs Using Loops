mid = "thub@gmail.com"
ipw = '1234'
for i in  range(5):
    umid = input('enter a mailid')
    upwd = input('enter a password')
    if mid == umid and ipw == upwd:
        print('log in successfully')
    else:
        print('wrong credentials')

#####

mid = "thub@gmail.com"
ipw = '1234'
for i in  range(4):
    umid = input('enter a mailid')
    upwd = input('enter a password')
    if mid == umid and ipw == upwd:
        print('log in successfully')
    else:
        print('wrong credentials')
else:
    print('account blocked')

#####

mid = "thub@gmail.com"
ipw = '1234'
for i in  range(3):
    umid = input('enter a mailid')
    upwd = input('enter a password')
    if mid == umid and ipw == upwd:
        print('log in successfully')
    else:
        print('wrong credentials')
else:
    print('account blocked')
print('all are done')

